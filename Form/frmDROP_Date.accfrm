'----------------------------
Private Function p_GetCtlInfoByName( _
    ctlName As String, _
    Optional Row As Byte, Optional Col As Byte, _
    Optional ctlType As calObjType = calObjTypeUndef _
    )
' получаем тип и номера строки и колонки по имени контрола
On Error GoTo HandleError
    Row = 0: Col = 0
    ctlType = calObjTypeUndef
    Select Case VBA.Left$(ctlName, 1)
    Case c_strColPref: ctlType = calObjTypeCol
    Case c_strRowPref: ctlType = calObjTypeRow
    Case c_strDayPref: ctlType = calObjTypeDay
    Case Else:         GoTo HandleExit
    End Select
    Row = CByte(VBA.Mid$(ctlName, 2, 1))
    Col = CByte(VBA.Mid$(ctlName, 3, 1))
HandleExit:  Exit Function
HandleError: Err.Clear: Resume HandleExit
End Function

Private Function p_GetCtlTypeByPos( _
    Row As Byte, Col As Byte, _
    Optional ctlType As calObjType = calObjTypeUndef, _
    Optional ctlName As String)
' получаем тип и имя контрола по номеру строки и колонки
Dim Result As Integer
    If Row = 0 And (Col >= 0 And Col <= cMax) Then
        ctlType = calObjTypeCol: ctlName = c_strColPref & Row & Col: Result = 0    ' c00 до с07 - заголовки колонок
    ElseIf Col = 0 And (Row > 0 And Row <= rMax) Then
        ctlType = calObjTypeRow: ctlName = c_strRowPref & Row & Col: Result = 0    ' r10 до r60 - заголовки строк
    ElseIf (Col > 0 And Col <= cMax) And (Row > 0 And Row <= rMax) Then
        ctlType = calObjTypeDay: ctlName = c_strDayPref & Row & Col: Result = 0    ' d11 до d67 - значения
    Else
        ctlType = calObjTypeUndef: Result = -1
    End If
HandleExit:  p_GetCtlTypeByPos = Result: Exit Function
HandleError: Err.Clear: Resume HandleExit
End Function
Private Function p_UpdateDays(Optional FirstDayOfWeek As VbDayOfWeek = vbMonday)
' заполняет календарь
Const c_strProcedure = "p_UpdateDays"
' ошибка при нумеровании недель. первая неделя если начало месяца приходится на выходной день считается неверно.
Dim Result As Boolean ':Result = False
    On Error GoTo HandleError
Dim r As Byte, c As Byte, d As Byte, f As Byte
Dim ctlColor As Long '
Dim strPrf As String * 1
Dim strCap As String, strDsc As String, bolHlt As Boolean
Dim eType As eDateType
Dim lngCol As Long
Dim bolVis As Boolean
'Dim DateType As eDateType
Dim strMessage As String, strComment As String
    BegDate = DateSerial(m_intYear, m_bytMonth, 1)
    EndDate = DateAdd("d", -1, DateAdd("m", 1, BegDate))
    LastDay = Day(EndDate)                  ' номер последнего дня
    d = 0                                   ' смещение относительно первого дня
    f = Weekday(BegDate, FirstDayOfWeek)    ' столбец первого дня
'    WindowFreeze Me.hWnd
    r = 0   ' строка
    Do      ' перебираем строки
        c = 0   ' колонка
        Do      ' перебираем колонки
            If r = 1 And c = f Then d = 1    ' соответствует дате месяца
            strCap = vbNullString: strDsc = vbNullString
            If r = 0 And c > 0 Then ' если первая строка - это строка заголовков дней недели
    ' день недели
                bolVis = True
                strPrf = c_strColPref
                strCap = WeekdayName(c, True, FirstDayOfWeek)           ' день недели
                strDsc = WeekdayName(c, False, FirstDayOfWeek)
                Select Case FirstDayOfWeek + c
                Case 8:                         lngCol = GetColorFromText(DateTypes(eDateTypeSatday, 4)):       bolHlt = True   ' суббота
                Case 2, 9:                      lngCol = GetColorFromText(DateTypes(eDateTypeSunday, 4)):       bolHlt = True   ' воскресенье
                Case Else:                      lngCol = GetColorFromText(DateTypes(eDateTypeWeekday, 4)):      bolHlt = False  ' рабочий
                End Select
            ElseIf r > 0 And c = 0 Then ' если первая колонка - это колонка заголовка номера недели
    ' номер недели
                bolVis = d <= LastDay ' выводим ячейку
                strPrf = c_strRowPref
                If bolVis Then
                    lngCol = GetColorFromText("DarkGray")
                    strCap = DatePart("ww", DateAdd("d", BegDate, d), , vbFirstJan1) ' номер недели
                    strDsc = strCap
                End If
            ElseIf r > 0 And c > 0 Then ' поле даты
    ' день месяца
                strPrf = c_strDayPref
                bolVis = (d >= 1) And (d <= LastDay) ' выводим ячейку
                If bolVis Then
                Dim strEventsId As String
                    strCap = d
                    eType = DateInfoGet(DateAdd("d", d - 1, BegDate), EventIds:=strEventsId, DateDesc:=strDsc)
                    bolHlt = (eType <> eDateTypeWeekday) Or Len(strEventsId)    ' выделяем если это нерабочий день или в есть событие
                    lngCol = GetColorFromText(DateTypes(eType, 4))              ' цвет даты в зависимости от типа
                    d = d + 1
                End If
            Else
                GoTo HandleNext
            End If
    ' Форматируем день месяца
            With Me.Controls(strPrf & r & c)
                If bolVis Then .Caption = strCap: .ControlTipText = strDsc: .ForeColor = lngCol: .FontBold = bolHlt
                .Visible = bolVis
            End With
HandleNext: c = c + 1
        Loop Until c > cMax
        r = r + 1
    Loop Until r > rMax
'    WindowUnFreeze 'Me.hWnd
    Result = True
HandleExit:  p_UpdateDays = Result: Exit Function
'HandleError: Stop: Err.Clear: Resume 0
HandleError: Result = False: Err.Clear: Resume HandleExit
End Function
Private Function p_Click( _
    Optional Row As Byte = 0, _
    Optional Col As Byte = 0)
' выделяет ячейку типа c_strDayPref в позиции (Row, Col)
Dim ctlType As calObjType, ctlName As String
Dim ctl As Access.Control ', box As Access.Control
Dim l As Long, t As Long, w As Long, h  As Long
Dim Typ As String
'Const Gap = 50
'    WindowFreeze Me.hWnd
    Call p_GetCtlTypeByPos(Row, Col, ctlType, ctlName)
    Set ctl = Me.Controls(ctlName)
    Select Case ctlType
     Case calObjTypeDay
     ' день - выделяем ячейку "d[r;c]"
        With ctl:       l = .Left:  t = .Top: w = .Width: h = .Height: End With
        With boxMark:   .Width = w: .Height = h: .Left = l: .Top = t: .Visible = True: End With
        boxLine.Visible = False
     Case calObjTypeRow
     ' строка - выделяем строку "r[r;0]:d[r;cMax]"
        With ctl:       t = .Top:   h = .Height:  End With '- 2 * b
        With boxGroup:  l = .Left:  w = .Width:     End With
        With boxLine:   .Width = cWidth: .Height = cHeight: .Left = l:   .Top = t: .Width = w: .Height = h: .Visible = True: End With
     Case calObjTypeCol
     ' колонка - выделяем колонку "с[0;r]:d[rMax;r]"
        With ctl:       l = .Left:  w = .Width:  End With
        With boxGroup:  t = .Top:   h = .Height: End With
        With boxLine:   .Width = cWidth: .Height = cHeight: .Left = l: .Top = t: .Width = w: .Height = h: .Visible = True: End With
     Case Else: boxLine.Visible = False
    End Select
'    WindowUnFreeze 'Me.hWnd
End Function
Private Function p_Hover( _
    Optional Show As Boolean = True, _
    Optional Row As Byte = 0, _
    Optional Col As Byte = 0)
' акцентирует ячейку типа c_strDayPref в позиции (Row, Col)
' если CtlType=calObjTypeUndef - снимает акцентирование у всех
Dim r As Byte, c As Byte
Dim ctlType As calObjType, ctlName As String
Dim ctl As Access.Control
Dim strStatus As String
'    WindowFreeze Me.hWnd
    strStatus = Space(1)
    Call SysCmd(acSysCmdSetStatus, " ")
    r = 0
    Do ' перебираем строки
        c = 0
        Do ' перебираем колонки
            If p_GetCtlTypeByPos(r, c, ctlType, ctlName) Then GoTo HandleNext
            Set ctl = Me.Controls(ctlName)
            With ctl
            If Show And (c = Col And r = Row) Then
            ' задано акцентировать и курсор над указаным элементом
            ' определяем как его акцентировать
                Select Case ctlType
                Case calObjTypeRow ' акцентируем строку (неделя)
'                    .SpecialEffect = 2  '
'                    .BorderStyle = 2
                    strStatus = VBA.Format$(m_varValue, "yyyy") & " - неделя №" & .ControlTipText
                    Call SysCmd(acSysCmdSetStatus, strStatus)
                Case calObjTypeCol ' акцентируем колонку (день недели)
'                    .SpecialEffect = 2  '
'                    .BorderStyle = 2
                    strStatus = VBA.Format$(m_varValue, "mmmm yyyy") & " - " & .ControlTipText
                    Call SysCmd(acSysCmdSetStatus, strStatus)
                Case calObjTypeDay ' акцентируем день
                    .SpecialEffect = 2: .BorderStyle = 2
                    strStatus = VBA.Format$(DateSerial(Year(m_varValue), Month(m_varValue), .Caption), "dd mmmm yyyy") & " - " & .ControlTipText
                    Call SysCmd(acSysCmdSetStatus, strStatus)
                    '.BorderColor
                Case Else ' снимаем акцентирование
                    .SpecialEffect = 0: .BorderStyle = 0
                End Select
             Else
             ' задано не акцентировать или курсор в другом месте - снимаем акцентирование
                If .Visible Then .SpecialEffect = 0: .BorderStyle = 0 'Flat/Transparent
            End If
            End With
HandleNext:
            c = c + 1
        Loop Until c > cMax
        r = r + 1
    Loop Until r > rMax
'    WindowUnFreeze 'Me.hWnd
End Function
Private Function p_FillMonthes()
Dim i As Byte
Dim strMonthes '"1;'Январь';2;'Февраль';3;'Март';4;'Апрель';5;'Май';6;'Июнь';7;'Июль';8;'Август';9;'Сентябрь';10;'Октябрь';11;'Ноябрь';12;'Декабрь'"
    strMonthes = vbNullString
    For i = 1 To 12
        strMonthes = strMonthes & c_strDelim & i & c_strDelim & "'" & MonthName(i) & "'"
    Next i
    strMonthes = Mid$(strMonthes, Len(c_strDelim) + 1)
    fldMonth.RowSource = strMonthes
End Function
