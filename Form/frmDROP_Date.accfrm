Private Sub p_SetValue(rData As Date)
Const cstrProcedure = "p_SetValue"
' Dim PrevDate as Date, NewDate as Date
    On Error GoTo HandleError
' Если разница между старой и новой датами меньше дня календарь обновлять не надо
    If DateDiff("d", rData, m_varValue) = 0 Then GoTo HandleExit
   
    m_bytDay = Day(rData)
    m_bytMonth = Month(rData): fldMonth = m_bytMonth
    m_intYear = Year(rData): fldYear = Format$(m_intYear, "###0")
    
    Caption = Format$(rData, "dddd, d mmmm yyyy г.")
' отмечаем текущий день
    'b = 1 * Pt
Dim f As Byte: f = Weekday(DateSerial(m_intYear, m_bytMonth, 1), FirstDayOfWeek)
    Call p_Click( _
        Row:=(f + m_bytDay - FirstDayOfWeek) \ 7 + 1, _
        Col:=Weekday(DateSerial(m_intYear, m_bytMonth, m_bytDay), FirstDayOfWeek))
' Если разница между старой и новой датами меньше месяца не обновляем календарь
    If DateDiff("m", rData, m_varValue) <> 0 Then p_UpdateDays
    m_varValue = rData
HandleExit:  Exit Sub
HandleError: Resume HandleExit
End Sub
