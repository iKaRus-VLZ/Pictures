        Case calObjTypeDay: CalDate = DateAdd("d", .Caption - 1, BegDate) ' d11 до d67 - значения
           Call p_Hover(Show:=True, Row:=Row, Col:=Col): If DateInfoAsk(CalDate, EventControl) Then p_UpdateDays      ' после изменения надо обновить вывод
        Case Else
        End Select
    End With
HandleExit: 'PrevX = x: PrevY = y
