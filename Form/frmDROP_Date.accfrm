Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
' не срабатывает событие
    On Error GoTo HandleError
    Select Case KeyCode
    Case vbKeyLeft:     If Shift = 0 Then p_SetValue DateAdd("d", -1, m_varValue)
    Case vbKeyRight:    If Shift = 0 Then p_SetValue DateAdd("d", 1, m_varValue)
    Case vbKeyUp:       If Shift = 0 Then p_SetValue DateAdd("ww", -1, m_varValue)
    Case vbKeyDown:     If Shift = 0 Then p_SetValue DateAdd("ww", 1, m_varValue)
    Case vbKeyPageUp:   If Shift = 0 Then cmdPrevMonth_Click Else If Shift = 1 Then cmdPrevYear_Click
    Case vbKeyPageDown: If Shift = 0 Then cmdNextMonth_Click Else If Shift = 1 Then cmdNextYear_Click
    Case vbKeyHome:     If Shift = 0 Then cmdToday_Click
    End Select
