Private Sub p_InitControl(ctl As Access.Control) ', Optional idx)
' добавляем групповой обработчик по контролу
Dim cTransf As clsControlEventsTransfer
    Set cTransf = New clsControlEventsTransfer
    cTransf.Init Index:=colCtls.Count, EventControl:=ctl, Relay:=mRelay
    colCtls.Add cTransf
End Sub
Private Sub p_InitControls()
' расставляем ячейки
Dim i As Long
Dim r As Byte, c As Byte
Dim w As Long, h As Long, l As Long, t As Long
Dim ctl As Access.Control ', cTransf As clsControlEventsTransfer
Dim strCtlName As String
Dim bytCtlType As calObjType
    Set ctl = Me.Controls(c_strColPref & 0 & 0)
    With ctl: w = cWidth: h = cHeight: l = .Left: t = .Top: End With
'    WindowFreeze Me.hWnd
' инициализируем коллекцию контролов
' для которых будет задана групповая обработка событий
    Set colCtls = New Collection
    r = 0 ' индекс первой строки

    On Error GoTo HandleNext
    Do Until r > rMax
        c = 0 ' индекс первой колонки
        Do Until c > cMax
        ' определяем тип контрола даты по номеру колонки и строки
        ' формируем имя и позицию контрола на форме
            If p_GetCtlTypeByPos(r, c, bytCtlType, strCtlName) Then GoTo HandleNext
        ' получаем искомый контрол и позиционируем его на форме
            Set ctl = Me.Controls(strCtlName)
            With ctl
        ' позициионируем контрол даты
                .Width = w: .Height = h
                Select Case bytCtlType
                Case calObjTypeCol: .Left = l + c * w: .Top = t                   ' c00 до с07 - заголовки колонок
                Case calObjTypeRow: .Left = l:         .Top = t + r * h + HeadGap ' r10 до r60 - заголовки строк
                Case calObjTypeDay: .Left = l + c * w: .Top = t + r * h + HeadGap ' d11 до d67 - значения
                Case Else: GoTo HandleNext
                End Select
            End With
            p_InitControl ctl ' инициализация групповой обработки событий
HandleNext:
        Err.Clear
            c = c + 1 ' следующая колонка
        Loop
        r = r + 1 ' следующая строка
    Loop
'    WindowUnFreeze
End Sub
Public Sub EventControl_Click( _
    EventControl As Access.Control) 'As Integer
' обработка события щелчок мышью для дат календаря
Dim Row As Byte, Col As Byte, ctlType As calObjType
Dim Result As Integer
    With EventControl
        p_GetCtlInfoByName .Name, Row, Col, ctlType
        Select Case ctlType
        Case calObjTypeCol: Call p_Click(Row, Col)  ' c00 до с07 - заголовки колонок
        Case calObjTypeRow: Call p_Click(Row, Col)  ' r10 до r60 - заголовки строк
        Case calObjTypeDay: Sel = .Caption          ' d11 до d67 - значения
                            If Sel < 1 Or Sel > LastDay Then GoTo HandleExit
                            DateDay = Sel: Call p_Click(Row, Col)
        Case Else
        End Select
    End With
HandleExit:
End Sub
